<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape>
            <corners
                android:radius="0dip" />
            <gradient
                android:startColor="#94BAFC"
                android:centerColor="#94BAFC"
                android:centerY="1"
                android:endColor="#94BAFC"
                android:angle="270" />
        </shape>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="0dip" />
                <gradient
                    android:startColor="#5A94E8"
                    android:endColor="#ff1997e1"
                    android:angle="90" />
            </shape>
        </clip>
    </item>
</layer-list>